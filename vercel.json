{
  "rewrites": [
    {
      "source": "/v1/:match*",
      "destination": "trello-be-production.up.railway.app/v1/:match*"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.(?:js|css|ico|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
